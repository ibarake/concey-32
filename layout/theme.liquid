<!doctype html>
<html lang="en">
  <head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <link
      rel="stylesheet"
      type="text/css"
      href="//cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.css"
    >
    <link
      rel="stylesheet"
      type="text/css"
      href="//cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick-theme.css"
    >
    <link href="http://fonts.googleapis.com/css?family=Lato&subset=latin,latin-ext" rel="stylesheet" type="text/css">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link
      href="https://fonts.googleapis.com/css2?family=Montserrat:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap"
      rel="stylesheet"
    >
    <style>
      @font-face {
        font-family: "Proxima nova bold";
        src: url("https://cdn.shopify.com/s/files/1/0514/6211/1387/files/Fontspring-DEMO-proximanova-bold.woff2?v=1693531731") format("woff2");
      }
      @font-face {
        font-family: "Proxima nova bold";
        src: url("https://cdn.shopify.com/s/files/1/0514/6211/1387/files/Fontspring-DEMO-proximanova-bold.woff2?v=1693531731") format("woff2");
      }
      @font-face {
        font-family: "Proxima nova medium";
        src: url("https://cdn.shopify.com/s/files/1/0514/6211/1387/files/Fontspring-DEMO-proximanova-medium.woff2?v=1693531742") format("woff2");
      }
      @font-face {
        font-family: "Proxima nova regular";
        src: url("https://cdn.shopify.com/s/files/1/0514/6211/1387/files/Fontspring-DEMO-proximanova-regular.woff2?v=1693531747") format("woff2");
      }
      @font-face {
        font-family: "Proxima nova thin";
        src: url("https://cdn.shopify.com/s/files/1/0514/6211/1387/files/Fontspring-DEMO-proximanova-thin.woff2?v=1693531753") format("woff2");
      }
      body {
        font-family: 'Proxima nova bold';
      }
      :root {
        --main-gray: #f3f3f3;
        --main-black: #000;
        --second-gray: #818181;
        --global-font-lato: Lato;
        --global-font-monserrat: Monserrat;
      }
    </style>
    {{ 'header.css' | asset_url | stylesheet_tag }}
    {{ 'product-grid.css' | asset_url | stylesheet_tag }}
    {{ 'footer.css' | asset_url | stylesheet_tag }}
    {{ 'about.css' | asset_url | stylesheet_tag }}
    {{ 'contact.css' | asset_url | stylesheet_tag }}
    {{ 'newsletter.css' | asset_url | stylesheet_tag }}
    <title>CONCEY 32</title>
    {{ content_for_header }}
  </head>
  <body>
    {% section 'header' %}
    {{ content_for_layout }}
    {% section 'about' %}
    {% section 'contact' %}
    {% section 'newsletter' %}
    {% section 'footer' %}
    <script type="text/javascript" src="//cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.min.js"></script>
    <script>
      $('.slick-carousel').slick({infinite: true, slidesToShow: 3, slidesToScroll: 3, arrows: false});
    </script>
  </body>
</html>
