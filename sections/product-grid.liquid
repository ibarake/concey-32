<product-grid> 
<head>
  <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
  <link rel="stylesheet" href="product-grid.css">
</head>
<section class="parallax">
  <div class="parallax-inner">
    <img src="https://images.squarespace-cdn.com/content/v1/56c34abb8259b52c41717077/1455914155783-BIEX5ZDYCW3W3KTNHKYD/home-2015-07-31-leather-model-01-0092-v3-FINAL.jpg?format=2500w.jpg" class="img-parallax" alt="">
</div>
  </div>
</section>



<script>
  window.addEventListener('scroll', function() {
    const parallax = document.querySelector('.img-parallax');
    const scrolled = window.scrollY;
    parallax.style.transform = `translateY(${scrolled * 0.5}px)`;
  });


</script>
</product-grid>



