<h1>FOOTER</h1>
<footer id="footer" class="cf">
  <nav id="footer-navigation" class="" data-content-field="navigation-footer-navigation">
    <div class="nav">
      <ul class="cf">
        {% # logic brackets %}
        {% for link in section.settings.footer-menu.links %}
          <li class="">
            {% # return of a value or object %}
            <a href="{{ link.url }}">{{ link.title }}</a>
          </li>
        {% endfor %}
      </ul>
    </div>
  </nav>
  {% # Social Media Icons %}
  <div class = "facebook-button">
    {% render 'facebook-icon' %}
  </div>
  <div class = "twitter-button">
    {% render 'twitter-icon' %}
  </div>
  <div class = "instagram-button">
    {% render 'instagram-icon' %}
  </div>
  
  <div
    class="sqs-layout sqs-grid-1 columns-1"
    data-layout-label="Left Footer Content"
    data-type="block-field"
    id="footerBlockLeft"
  >
    <div class="row sqs-row">
      <div class="col sqs-col-1 span-1">
        <div class="sqs-block html-block sqs-block-html" data-block-type="2" id="block-8864d463d34c73def6ca">
          <div class="sqs-block-content">
            <div class="sqs-html-content"></div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="sqs-layout sqs-grid-1 columns-1" data-layout-label="Right Footer Content" data-type="block-field" id="footerBlockRight">
    <div class="row sqs-row">
        <div class="col sqs-col-1 span-1">
            <div class="sqs-block socialaccountlinks-v2-block sqs-block-socialaccountlinks-v2" data-block-type="54" id="block-f89c2d4ff9e83cab80a7">
                <div class="sqs-block-content">
                    <nav class="sqs-svg-icon--list">
                        <a href="#" target="_blank" class="sqs-svg-icon--wrapper twitter" aria-label="">
                            <div>
                                <svg class="sqs-svg-icon--social" viewBox="0 0 64 64">
                                    <use class="sqs-use--icon" xlink:href="#twitter-icon"></use>
                                    <use class="sqs-use--mask" xlink:href="#twitter-mask"></use>
                                </svg>
                            </div>
                        </a>
                        <a href="#" target="_blank" class="sqs-svg-icon--wrapper facebook" aria-label="">
                            <div>
                                <svg class="sqs-svg-icon--social" viewBox="0 0 64 64">
                                    <use class="sqs-use--icon" xlink:href="#facebook-icon"></use>
                                    <use class="sqs-use--mask" xlink:href="#facebook-mask"></use>
                                </svg>
                            </div>
                        </a>
                        <a href="#" target="_blank" class="sqs-svg-icon--wrapper instagram" aria-label="">
                            <div>
                                <svg class="sqs-svg-icon--social" viewBox="0 0 64 64">
                                    <use class="sqs-use--icon" xlink:href="#instagram-icon"></use>
                                    <use class="sqs-use--mask" xlink:href="#instagram-mask"></use>
                                </svg>
                            </div>
                        </a>
                    </nav>
                </div>
            </div>
        </div>
    </div>
</div>
</footer>

{% # id must have "-" between words %}

{% schema %}
{
  "name": "Footer",
  "settings": [
    {
      "type": "link_list",
      "id": "footer-menu",
      "label": "Menu"
    }
  ]
}
{% endschema %}

{% stylesheet %}
{% endstylesheet %}

{% javascript %}
{% endjavascript %}
